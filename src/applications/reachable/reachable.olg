materialize(link,infinity,infinity,keys(1,2)).
materialize(path,infinity,infinity,keys(4:list)).

r1 path(@X,Y,C,P) :- link(@X,Y,C), 
		      P1:=f_append(X), 
		      P2:=f_append(Y),
		      P:=f_concat(P1,P2). 

r2 path(@X,Y,C,P) :- link(@X,Z,C1), 
		     path(@Z,Y,C2,P2), 
		     C:=C1+C2, 
		     f_member(P2,X)==0,
		     P1:=f_append(X),
		     P:=f_concat(P1,P2).

